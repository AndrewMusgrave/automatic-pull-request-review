{
  "name": "automatic-pull-request-review",
  "version": "0.0.2",
  "description": "üëç Github action to automate pull requests",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "watch": "ncc build index.ts --watch --minify",
